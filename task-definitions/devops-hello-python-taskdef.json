[
    {
      "name": "${container_name}",
      "image": "${aws_ecr_repository}:${tag}",
      "essential": true,      
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "cpu": 1,
      "environment": [
        {
          "name": "ENV",
          "value": "PROD"
        }
      ],
      "ulimits": [
        {
          "name": "nofile",
          "softLimit": 65536,
          "hardLimit": 65536
        }
      ],
      "mountPoints": [],
      "memory": 512,
      "volumesFrom": []
    }
  ]
  